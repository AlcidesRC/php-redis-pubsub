{
    "name": "alcidesrc/php-redis-pubsub",
    "description": "Redis PUB/SUB implementation based on PHP, Predis and Monolog.",
    "keywords": [
        "php",
        "predis",
        "monolog",
        "redis",
        "pubsub",
        "domain events"
    ],
    "type": "library",
    "license": ["MIT"],
    "homepage": "https://github.com/alcidesrc/php-redis-pubsub",
    "authors": [
        {
            "name": "Alcides Ramos",
            "email": "info@alcidesramos.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "predis/predis": "^1.1",
        "monolog/monolog": "^2.2",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.82",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^9.5",
        "codedungeon/phpunit-result-printer": "^0.30.1"
    },
    "autoload": {
        "psr-4": {
            "PhpRedisPubSub\\": "src/PhpRedisPubSub/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpRedisPubSub\\Test\\": "test/PhpRedisPubSub/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.3"
        },
        "sort-packages": true
    }
}
